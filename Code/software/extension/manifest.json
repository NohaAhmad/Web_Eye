{

  "manifest_version": 2,
  "name": "Web-Eyes",
  "version": "1.0",

  "description": "Caption the Images Inside Web Pages",

  "icons": {
    "48": "icons/border-48.png"
  },


  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["webeyes.js"]
    }
  ],
  
  "permissions": [
    "webRequest",
    "*://127.0.0.1/*"

]
}
